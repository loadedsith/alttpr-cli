{
  "name": "alttpr-cli",
  "version": "1.0.0",
  "description": "",
  "main": "run.js",
  "repository": "github:loadedsith/alttpr-cli",
  "bin": {
    "alttpr-cli": "./run.js"
  },
  "scripts": {
    "start": "node run.js",
    "test": "jasmine",
    "test-inspect": "node --inspect-brk  ./node_modules/jasmine/bin/jasmine.js",
    "watch-test-inspect": "nodemon --ignore bundle.js --ext js,jade --exec 'npm run test-inspect'",
    "watch-test": "nodemon --ignore bundle.js --ext js,jade --exec 'npm run test'",
    "watch": "nodemon --ignore bundle.js --ext js,jade --exec 'npm run build'",
    "download-sprites": "node downloadSprites.js",
    "postinstall": "./updateSprites.sh",
    "update-parent-version": "git rev-parse HEAD > .parent-version",
    "precommit": ["test", "update-parent-version"]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "file-api": "^0.10.4",
    "file-saver": "^2.0.0",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "spark-md5": "^3.0.0",
    "yargs": "^13.2.0"
  },
  "devDependencies": {
    "nock": "^10.0.6",
    "pre-commit": "^1.2.2"
  }
}
